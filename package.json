{
  "name": "electron-angular-boilerplate",
  "version": "1.0.0",
  "description": "electron-angular-boilerplate",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtham8/electron-angular-boilerplate.git"
  },
  "author": "Michelle Tham",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "gulp watch",
    "build": "export NODE_ENV=production && electron-packager ./build --out=dist --app-version=$npm_package_version --prune --overwrite --arch=ia32 --arch=x64 --platform=darwin --platform=win32",
    "scss": "node-sass --output-style compressed -o ./css ./sass",
    "watch:css": "onchange 'sass/*.scss' -- npm run scss"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "bugs": {
    "url": "https://github.com/mtham8/electron-angular-boilerplate/issues"
  },
  "homepage": "https://github.com/mtham8/electron-angular-boilerplate#readme",
  "dependencies": {
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "bower": "^1.7.9",
    "browser-sync": "^2.14.0",
    "devtron": "^1.3.0",
    "electron-debug": "^1.0.1",
    "electron-localshortcut": "^0.6.1",
    "electron-packager": "^7.4.0",
    "electron-prebuilt": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^2.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-exec": "^2.1.2",
    "gulp-if": "^2.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-main-bower-files": "^1.5.2",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-useref": "^3.1.0",
    "gulp-xo": "^0.12.0",
    "nedb": "^1.8.0",
    "node-sass": "^3.8.0",
    "npm-main-files": "^1.0.0",
    "onchange": "^2.5.0",
    "xo": "^0.16.0",
    "xtend": "^4.0.1"
  }
}
